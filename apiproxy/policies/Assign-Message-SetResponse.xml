<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Assign-Message-SetResponse">
    <DisplayName>Assign Message-SetResponse</DisplayName>
    <Properties/>
    <Set>
        <Payload contentType="application/json">{"geolink":"https://maps.google.com/?q={lat},{lng}","address":"{request.queryparam.address}"}</Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>